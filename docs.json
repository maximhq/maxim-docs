{
    "$schema": "https://mintlify.com/docs.json",
    "theme": "palm",
    "name": "Mint Starter Kit",
    "colors": {
        "primary": "#0C3B43",
        "light": "#07C983"
    },    
    "appearance": {        
        "default": "light",
        "strict": true
    },
    "favicon": "/favicon.svg",
    "navigation": {        
        "tabs": [
            {
                "tab": "Documentation",
